#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
   int arr[]={33,45,21,1,99};
   int n=5;
   sort(arr,arr+n);
   for(int x:arr)
   {
       cout<<x<<" ";
   }
   cout<<endl;
  sort(arr,arr+n,greater<int>());
  for(int x:arr)
   {
       cout<<x<<" ";
   }
    return 0;
}
