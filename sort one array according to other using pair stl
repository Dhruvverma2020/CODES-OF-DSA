#include<bits/stdc++.h>
using namespace std;
void makepair(int a[],char b[],int n)
{
    pair<int,char>pa[n];
    for(int i=0;i<n;i++)
    {
        pa[i]={a[i],b[i]};
    }
    sort(pa,pa+n);
    for(int i=0;i<n;i++)
    {
        cout<<pa[i].second<<" ";
    }
}
int main()
{
    int a[]={3,1,2};
    char b[]={'G','E','K'};
    int n=3;
    makepair(a,b,n);
}
