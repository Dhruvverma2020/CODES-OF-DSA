#include <iostream>
#include<bits/stdc++.h>
using namespace std;
struct node
{
    node*left;
    node*right;
    int key;
    node(int k)
    {
        left=NULL;
        right=NULL;
        key=k;
    }
};
void preorder(node*root)
{
    
    if(root!=NULL)
    {
       preorder(root->left);
       preorder(root->right);
       cout<<root->key<<" ";
    }
}
int main()
{
    node*root=new node(10);
    root->left=new node(5);
    root->left->left=new node(1);
    root->right=new node(15);
    root->right->right=new node(20);
    preorder(root);
    return 0;
}
