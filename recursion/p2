#include<bits/stdc++.h>
using namespace std;
int sums(int n)
{
    
    if(n==0)
    return 0;
    
    return n+sums(n-1);
}
/*int add(int n,int sum)
{
    if(n<1)
    return sum;
    add(n-1,sum+n);
}*/
int fact(int n)
{
    if(n==0||n==1)
    return 1;
    
    return n=n*fact(n-1);
}
int main()
{
    int n=5;
    int idx=0;
    //cout<<sums(n,idx);
    //cout<<sums(5);
    cout<<fact(5);
}
