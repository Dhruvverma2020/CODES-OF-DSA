#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int rem=0;
    int temp=n;
    while(n!=0)
    {
        rem=rem*10+n%10;
        n=n/10;
        
    }
   cout<<rem<<endl;
   if(rem==temp)
   {
       cout<<"yes";
   }
   else
   {
       cout<<"no";
   }
    return 0;
}
