#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int maximum(int arr[],int n)
{
    int max=arr[0];
    for(int i=1;i<n;i++)
    {
        if(arr[i]>max)
        {
            max=arr[i];
        }
        
    }
    return max;
}
int main()
{
    int arr[]={34,56,7,89,110,22,11};
    int n=7;
    cout<<maximum(arr,7);

    return 0;
}
