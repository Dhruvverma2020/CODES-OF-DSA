#include<bits/stdc++.h>
using namespace std;
int main()
{
    unordered_map<int,int>umap;
    int arr[]={10,12,10,15,10,20,12,12};
    int n=8;
    for(int i=0;i<n;i++)
    {
        umap[arr[i]]++;
    }
    for(auto it=umap.begin();it!=umap.end();it++)
    {
        cout<<it->first<<" "<<it->second<<endl;
    }
    
}
