#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
   int arr[]={8,7,7,8,8};
   
   int n=5;
   for(int i=0;i<n;i++)
   {
       int count=0;
       for(int j=0;j<n;j++)
       {
           if(arr[j]==arr[i])
           count++;
            
       }
       if(count%2!=0)
           cout<<arr[i];
           break;
   }
    return 0;
}
